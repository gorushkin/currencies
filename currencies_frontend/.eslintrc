{
  "parser": "@typescript-eslint/parser",
  "extends": [
    "react-app",
    "prettier",
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "plugin:perfectionist/recommended-alphabetical"
  ],
  "rules": {
    "no-console": "warn",
    "react-hooks/exhaustive-deps": "error",
    "@typescript-eslint/no-empty-function": "warn",
    "max-len": [
      "warn",
      {
        "code": 120
      }
    ]
  }
}
